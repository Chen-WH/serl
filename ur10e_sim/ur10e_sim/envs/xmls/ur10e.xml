  <!-- 添加2f85夹爪的contact排除规则 -->
  <contact>
    <exclude body1="base" body2="left_driver"/>
    <exclude body1="base" body2="right_driver"/>
    <exclude body1="base" body2="left_spring_link"/>
    <exclude body1="base" body2="right_spring_link"/>
    <exclude body1="right_coupler" body2="right_follower"/>
    <exclude body1="left_coupler" body2="left_follower"/>
  </contact>
  <!-- 添加2f85夹爪的tendon、equality、actuator配置 -->
  <tendon>
    <fixed name="split">
      <joint joint="right_driver_joint" coef="0.5"/>
      <joint joint="left_driver_joint" coef="0.5"/>
    </fixed>
  </tendon>
  <equality>
    <connect anchor="0 0 0" body1="right_follower" body2="right_coupler" solimp="0.95 0.99 0.001" solref="0.005 1"/>
    <connect anchor="0 0 0" body1="left_follower" body2="left_coupler" solimp="0.95 0.99 0.001" solref="0.005 1"/>
    <joint joint1="right_driver_joint" joint2="left_driver_joint" polycoef="0 1 0 0 0" solimp="0.95 0.99 0.001"
      solref="0.005 1"/>
  </equality>
  <actuator>
    <general class="size4" name="shoulder_pan" joint="shoulder_pan_joint"/>
    <general class="size4" name="shoulder_lift" joint="shoulder_lift_joint"/>
    <general class="size3_limited" name="elbow" joint="elbow_joint"/>
    <general class="size2" name="wrist_1" joint="wrist_1_joint"/>
    <general class="size2" name="wrist_2" joint="wrist_2_joint"/>
    <general class="size2" name="wrist_3" joint="wrist_3_joint"/>
    <!-- ...existing code... -->
    <general class="2f85" name="fingers_actuator" tendon="split" forcerange="-5 5" ctrlrange="0 255"
      gainprm="0.3137255 0 0" biasprm="0 -100 -10"/>
  </actuator>
  <!-- 可选：添加2f85相关的传感器 -->
  <sensor>
    <jointpos name="size4/shoulder_pan_joint_pos" joint="shoulder_pan_joint"/>
    <jointpos name="size4/shoulder_lift_joint_pos" joint="shoulder_lift_joint"/>
    <jointpos name="size3_limited/elbow_joint_pos" joint="elbow_joint"/>
    <jointpos name="size2/wrist_1_joint_pos" joint="wrist_1_joint"/>
    <jointpos name="size2/wrist_2_joint_pos" joint="wrist_2_joint"/>
    <jointpos name="size2/wrist_3_joint_pos" joint="wrist_3_joint"/>

    <jointvel name="size4/shoulder_pan_joint_vel" joint="shoulder_pan_joint"/>
    <jointvel name="size4/shoulder_lift_joint_vel" joint="shoulder_lift_joint"/>
    <jointvel name="size3_limited/elbow_joint_vel" joint="elbow_joint"/>
    <jointvel name="size2/wrist_1_joint_vel" joint="wrist_1_joint"/>
    <jointvel name="size2/wrist_2_joint_vel" joint="wrist_2_joint"/>
    <jointvel name="size2/wrist_3_joint_vel" joint="wrist_3_joint"/>

    <torque name="size4/shoulder_pan_joint_torque" site="shoulder_pan_joint"/>
    <torque name="size4/shoulder_lift_joint_torque" site="shoulder_lift_joint"/>
    <torque name="size3_limited/elbow_joint_torque" site="elbow_joint"/>
    <torque name="size2/wrist_1_joint_torque" site="wrist_1_joint"/>
    <torque name="size2/wrist_2_joint_torque" site="wrist_2_joint"/>
    <torque name="size2/wrist_3_joint_torque" site="wrist_3_joint"/>

    <!-- ...existing code... -->
    <framepos name="2f85/pinch_pos" objtype="site" objname="pinch"/>
    <framequat name="2f85/pinch_quat" objtype="site" objname="pinch"/>
    <framelinvel name="2f85/pinch_vel" objtype="site" objname="pinch"/>
  </sensor>
  <!-- ...existing code... -->
</mujoco>
